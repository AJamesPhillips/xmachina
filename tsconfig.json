{
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "target": "es5",
    "module": "ES2015",
    "noImplicitAny": true,
    "declarationDir": "dist/types",
    "esModuleInterop": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "outDir": "dist"
  },
  "include": [
    "src"
  ],
  "types": [
    "typePatches",
    "node",
    "webpack-env",
    "type"
  ]
}